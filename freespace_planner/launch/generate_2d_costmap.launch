<launch>
  <!--
  <rosparam file="$(find freespace_planner)/cfg/costmap_cfg.yaml" command="load" ns="/costmap_node/costmap" /> -->
  <rosparam file="$(find racecar_nav)/cfg/costmap_common_params.yaml" command="load" ns="local_costmap" />
  <rosparam file="$(find racecar_nav)/cfg/local_costmap_params.yaml" command="load" />
  <node pkg="costmap_2d" type="costmap_2d_node" name="local_costmap">
    <rosparam>
      global_frame: odom
      robot_base_frame: base_link


      update_frequency: 30
      rolling_window: true

      width: 10
      height: 10
      resolution: 0.05

    </rosparam>
  </node>
</launch>